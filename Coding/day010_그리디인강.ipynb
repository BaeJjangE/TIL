    "N, K = map(int, input().split())\n",
    "a = 0\n",
    "count = 0\n",
    "while 1:\n",
    "    if N ==1:\n",
    "        break\n",
    "    elif N % K != 0:\n",
    "        N -= 1\n",
    "        count += 1\n",
    "    else:\n",
    "        N /= K\n",
    "        count += 1\n",
    "\n",
    "print(count)\n",
    "%time"
   ]
